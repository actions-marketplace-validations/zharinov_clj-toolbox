{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^\\.github\\/workflows\\/.*\\.yml$"],
      "matchStrings": ["[\"']?(?<currentValue>[^\"\\s]*)[\"']?\\s*#\\s*renovate-graalvm"],
      "extractVersion": "^vm-(?<version>\\d+\\.\\d+\\.\\d+)$",
      "datasourceTemplate": "github-releases"
    }
  ]
}
